{
  "name": "able2",
  "version": "1.2.4",
  "description": "Able2 is an all-in-one News Editor's toolkit. Able2 is able to download video from the internet, do screen records and convert and process video for air.",
  "main": "build/main.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "cleanup": "cd build/renderer && rimraf global.bundle.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "rimraf build && webpack --config webpack.prod.js && npm run cleanup",
    "pack": "rimraf dist && electron-builder --dir",
    "dist": "rimraf dist && npm run build && electron-builder -w -m",
    "deploy": "rimraf dist && npm run build && electron-builder build -w -m --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "update-ytdl:mac": "node_modules/youtube-dl-ffmpeg-ffprobe-static/bin/darwin/x64/youtube-dl -U",
    "update-ytdl:win": "cd node_modules/youtube-dl-ffmpeg-ffprobe-static/bin/win32/ia32 && youtube-dl.exe -U && cd ../x64 && youtube-dl.exe -U"
  },
  "author": "Jonathan Hamilton",
  "license": "CC-BY-NC-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JHWebDeveloper/Able2-public-resources.git"
  },
  "dependencies": {
    "electron-log": "^4.0.3",
    "electron-updater": "^4.2.0",
    "electron-util": "^0.12.3",
    "ffmpeg-static-electron": "^2.0.1",
    "ffprobe-static-electron": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-transition-group": "^1.2.1",
    "to-css": "^1.2.1",
    "uuid": "^3.3.3",
    "youtube-dl-ffmpeg-ffprobe-static": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-react": "^7.8.0",
    "@babel/register": "^7.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "electron": "^7.1.9",
    "electron-builder": "^21.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "postcss": "^7.0.26",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^2.7.1",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": "electron 6.0.1"
}
